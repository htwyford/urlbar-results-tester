{
  "manifest_version": 2,
  "name": "Firefox Address Bar Results Tester",
  "version": "1.0.0",
  "description": "Creates one result of each type in the Firefox address bar for testing purposes.",
  "applications": {
    "gecko": {
      "id": "urlbar-results-tester@mozilla.org",
      "strict_min_version": "70.0"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "urlbar-results-tester@mozilla.org",
      "strict_min_version": "70.0"
    }
  },
  "browser_action": {
    "default_icon": "icons/sort.svg",
    "default_title": "Urlbar Results Spoofer"
  },
  "permissions": [
    "urlbar"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "incognito": "spanning",
  "hidden": true
}
